# DB Configuration
DB_HOST: localhost
DB_NAME: esign_db
DB_PORT: 5435
DB_USERNAME: postgres
DB_PASSWORD: 1234


spring:
  flyway:
    enabled: false
  datasource:
    url: jdbc:postgresql://${DB_HOST}:${DB_PORT}/${DB_NAME}
    driver-class-name: org.postgresql.Driver
    username: ${DB_USERNAME}
    password: ${DB_PASSWORD}
server:
  port: 8081
